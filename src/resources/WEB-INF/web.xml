<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>java_echo</servlet-name>
        <servlet-class>java_echo.EchoServlet</servlet-class>
	</servlet>
    <servlet-mapping>
        <servlet-name>java_echo</servlet-name>
        <url-pattern>/java_echo</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>scala_echo</servlet-name>
        <servlet-class>scala_echo.EchoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>scala_echo</servlet-name>
        <url-pattern>/scala_echo</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>scala_taskqueue</servlet-name>
        <servlet-class>scala_taskqueue.TaskQueueServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>scala_taskqueue</servlet-name>
        <url-pattern>/scala_taskqueue</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>runnabletask</servlet-name>
        <servlet-class>runnabletask.RunnableTaskServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>runnabletask</servlet-name>
        <url-pattern>/tasks/RunnableTaskServlet</url-pattern>
    </servlet-mapping>
</web-app>
